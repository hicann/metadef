<?xml version="1.0" encoding="UTF-8"?>
<config name="metadef" copy_type="delivery" src_path="" dst_path="metadef" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>metadef</func_name>
        <suffix>run</suffix>
        <install_script>share/info/metadef/script/install.sh</install_script>
        <help>share/info/metadef/script/help.info</help>
        <version>package/metadef/version.xml</version>
        <cleanup>share/info/metadef/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
    </package_info>
    <generate_info value="metadef_version.h" dst_path="share/info/metadef" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
       <METADEF_VERSION>$(ASCEND_VER)</METADEF_VERSION>
       <METADEF_TIMESTAMP>$(TIMESTAMP_NO)</METADEF_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="share/info/metadef" install_path="share/info/metadef" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <version_info install="true" install_path="metadef" install_own="$username:$usergroup" install_mod="440"/>
    <file_info value="metadef_script" copy_type="source" dst_path="share/info/metadef" install_path="share/info/metadef">
        <file value="script" entity="true"/>
        <file value="version.info"/>
    </file_info>
    <block_info dependtree="false" dst_path="metadef" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
        <block name="DevlibCommon"/>
        <block name="MetadefSo"/>
        <block name="MetadefInc"/>
    </block_info>
    <dir_info value="metadef" install_path="metadef" install_type="all">
        <path value="share" install_mod="750"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/metadef" install_mod="750"/>
        <path value="share/info/metadef/script" install_mod="550"/>
        <path value="runtime" install_mod="550"/>
        <path value="runtime/include" install_mod="550"/>
        <path value="runtime/include/graph" install_mod="550"/>
    </dir_info>
</config>
